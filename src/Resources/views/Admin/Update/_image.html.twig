{% if data.image is not empty %}
    {% set path = data.image.path|imagine_filter(filter|default('sylius_admin_product_small_thumbnail')) %}
{% else %}
    {% set path = asset('assets/admin/img/50x50.png') %}
{% endif %}

<img src="{{ path }}" alt="{{ data.title }}" title="{{ data.title }}" class="ui bordered image sylius-grid-image" />
